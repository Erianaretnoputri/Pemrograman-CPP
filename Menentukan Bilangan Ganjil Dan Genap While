#include <bits/stdc++.h>
#include <iostream>

using namespace std;

int main() {
	
	int x = 1;
	int size;
	
	cin >> size;
	
	//Menentukan Bilangan Ganjil
	cout << "Deretan Bilangan Ganjil adalah : ";
	while(x <= size) {
		
		if(x%2 == 1) {
			cout << x << " ";
		}
		
		x++;
	}
	
	cout << endl;
	
	//Menentukan Bilangan Genap
	int y = 1;
	cout << "Deretan Bilangan Genap adalah : ";
	while (y <= size) {
		
		if(y%2 == 0) {
			cout << y << " ";
		}
		
		y++;
	}
	
}
